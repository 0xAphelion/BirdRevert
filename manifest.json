{
	"name": "BirdRevert",
	"manifest_version": 3,
	"version": "0.0.1",
	"description": "Bring back the bird",
	"action": {
		"default_icon": {
			"16": "icons/icon16.png",
			"24": "icons/icon24.png",
			"32": "icons/icon32.png"
		}
	},
	"icons": {
		"16": "icons/icon16.png",
		"24": "icons/icon24.png",
		"32": "icons/icon32.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
	},
	"content_scripts": [
		{
		"matches": [
			"*://twitter.com/*",
			"*://x.com/*"
		],
		"js": ["content.js"],
		"run_at": "document_end"
		}
	],
	"background": {},
	"permissions": [
		"activeTab",
		"scripting"
	],
	"host_permissions": [
		"*://twitter.com/*",
		"*://x.com/*"
	],
	"web_accessible_resources": [{
		"resources": ["*"],
		"matches": ["*://*/*"],
		"extension_ids": ["*"]
	  }]
}